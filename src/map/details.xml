<?xml version="1.0" encoding="ISO-8859-1" ?>
<root>
	<screen>
		<htmltempalte>
			pages/detailstpl.xml
		</htmltempalte>
		<includedjsp>

		</includedjsp>
		<scripts>

			<scriptinclude>js/test.js,js/jquery.js</scriptinclude>
		</scripts>
		<stylesheets>

			<styleinclude>css/home.css</styleinclude>
		</stylesheets>
	</screen>

	<panels>
		<fields>
			<field>
				<display type="text" class="userid" forid="userid" value="${firstname}"
					name="firstname" id="firstname" column="USER_ID" rulemessage=""
					dbcolsize="" mask="" mandatory="" hidden="">
				</display>
			</field>

			<field>
				<display type="text" class="password" forid="password"
					value="${LastName}" name="LastName" id="LastName" validationattr=""
					column="PASSWD" rulemessage="" dbcolsize="" mask="">
				</display>
			</field>
			<field>
				<display type="text" class="password" forid="dateid"
					value="${orderdate}" name="LastName" id="LastName" validationattr=""
					column="PASSWD" rulemessage="" dbcolsize="" mask="">
				</display>
			</field>
			<field>
				<display type="text" class="password" forid="gender"
					value="${gender}" name="LastName" id="LastName" validationattr=""
					column="PASSWD" rulemessage="" dbcolsize="" mask="">
				</display>
			</field>
			<field>
				<display type="text" class="password" forid="list"
					value="${country}" name="LastName" id="LastName" validationattr=""
					column="PASSWD" rulemessage="" dbcolsize="" mask="">
				</display>
			</field>
			<field>
				<display type="text" class="password" forid="check"
					value="${check}" name="LastName" id="LastName" validationattr=""
					column="PASSWD" rulemessage="" dbcolsize="" mask="">
				</display>
			</field>
			
			
		</fields>
		
	</panels>
</root>